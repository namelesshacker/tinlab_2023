clock x;

id_t list[N+1];
int[0,N] len;

void enqueue(id_t element)
{
	list[len++] = element;
}

void dequeue()
{
	int i =0;
	len -= 1;
	while(i &lt; len)
	{
		list[i] = list[i+1];
		i++;
	}
	list[i]=0;
}
// Returns the front element of the queue
id_t front()
{
   return list[0];
}

// Returns the last element of the queue
id_t tail()
{
   return list[len - 1];
}


bool check()
{
